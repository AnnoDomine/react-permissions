{
    "name": "@derhacki/react-permissions",
    "version": "0.2.1",
    "description": "Permission controll",
    "scripts": {
        "prepack": "npm build",
        "tsc": "npx tsc && npx tsc --module commonjs --outDir lib/cjs",
        "build": "rollup -c",
        "watch": "rollup -cw",
        "release": "npm run build && lerna publish --git-remote react-permissions"
    },
    "author": "Dominic Seel <seel.dominic@googlemail.com>",
    "license": "MIT",
    "dependencies": {
        "components": "*",
        "hooks": "*",
        "provider": "*"
    },
    "main": "./lib/cjs/index.js",
    "module": "./lib/esm/index.js",
    "types": "./lib/esm/index.d.ts",
    "files": [
        "/lib"
    ],
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^24.0.0",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-typescript": "^11.0.0",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.10",
        "node-sass": "^8.0.0",
        "postcss": "^8.4.21",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rollup": "^3.10.0",
        "rollup-plugin-dts": "^5.1.1",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-typescript2": "^0.34.1",
        "typescript": "^4.9.4"
    },
    "nx": {
        "targets": {
            "build": {
                "outputs": [
                    "{projectRoot}/build",
                    "{projectRoot}/public/build"
                ]
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/AnnoDomine/react-permissions"
    },
    "bugs": {
        "url": "https://github.com/AnnoDomine/react-permissions/issues"
    },
    "gitHead": "35b86c48e4b0f64f21037d236179a3d25a3a4a9e"
}
